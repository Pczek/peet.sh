---
export interface Props {
  title: string;
}
import 'latex.css';
const { title } = Astro.props;
---

<html>
  <head>

    <title>{title}</title>
  </head>
  <body>


    <main>
      <article>
        <h1>{title}</h1>
        <slot />
        <a href="/"><p>Back</p></a>
      </article>
    </main>
  </body>
</html>
